    "@vitest/coverage-v8": "^0.34.4",
    "@vitest/ui": "^0.34.4",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "happy-dom": "^11.0.6",
    "jsdom": "^22.1.0",
